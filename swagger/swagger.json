{
    "swagger": "2.0",
    "info": {
        "title": "beego Test API",
        "description": "beego has a very cool tools to autogenerate documents for your API",
        "version": "1.0.0",
        "termsOfService": "http://beego.me/",
        "contact": {
            "email": "astaxie@gmail.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    },
    "basePath": "/v1",
    "paths": {
        "/login/login": {
            "post": {
                "tags": [
                    "login"
                ],
                "description": "用户登录",
                "operationId": "LoginController.用户登录",
                "parameters": [
                    {
                        "in": "formData",
                        "name": "email",
                        "description": "邮箱",
                        "required": true,
                        "type": "string",
                        "default": "demo@qq.com"
                    },
                    {
                        "in": "formData",
                        "name": "password",
                        "description": "密码",
                        "required": true,
                        "type": "string",
                        "default": "123"
                    }
                ],
                "responses": {
                    "1000": {
                        "description": "参数错误"
                    },
                    "1001": {
                        "description": "请求出错"
                    },
                    "200": {
                        "description": "登录成功"
                    },
                    "2001": {
                        "description": "用户不存在"
                    },
                    "2002": {
                        "description": "用户名或者密码错误"
                    }
                }
            }
        },
        "/logout/logout": {
            "post": {
                "tags": [
                    "logout"
                ],
                "description": "用户登出",
                "operationId": "LogoutController.用户登出",
                "responses": {
                    "200": {
                        "description": "登出成功"
                    }
                }
            }
        },
        "/signup/": {
            "post": {
                "tags": [
                    "signup"
                ],
                "description": "Register",
                "operationId": "RegisterController.Register",
                "parameters": [
                    {
                        "in": "formData",
                        "name": "username",
                        "description": "用户名",
                        "required": true,
                        "type": "string",
                        "default": "ant"
                    },
                    {
                        "in": "formData",
                        "name": "password",
                        "description": "密码",
                        "required": true,
                        "type": "string",
                        "default": "123"
                    },
                    {
                        "in": "formData",
                        "name": "email",
                        "description": "邮箱",
                        "required": true,
                        "type": "string",
                        "default": "demo@qq.com"
                    }
                ],
                "responses": {
                    "1000": {
                        "description": "参数错误"
                    },
                    "1001": {
                        "description": "请求出错"
                    },
                    "2000": {
                        "description": "用户名已存在"
                    },
                    "2005": {
                        "description": "邮箱已存在"
                    }
                }
            }
        },
        "/user/": {
            "get": {
                "tags": [
                    "user"
                ],
                "description": "获取用户列表的",
                "responses": {
                    "1102": {
                        "description": "请求出错"
                    },
                    "200": {
                        "description": "请求成功"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "user",
            "description": "Operations about Users\n"
        },
        {
            "name": "login",
            "description": "Operations about Login\n"
        },
        {
            "name": "logout",
            "description": "Operations about Logout\n"
        },
        {
            "name": "signup",
            "description": "Operations about Register\n"
        }
    ]
}